            <characteristic name="射程" typeId="5bf6-378a-0cb7-b079">18寸</characteristic>
            <characteristic name="S" typeId="12da-9b3e-f37b-bc35">6</characteristic>
            <characteristic name="AP" typeId="10b5-aa5b-ccde-79cc">-</characteristic>
