      <profiles>
        <profile id="254b-5059-bde3-4eac" name="Primus Redoubt" hidden="false" typeId="286c-0cd4-7630-47d0" typeName="建筑">
          <characteristics>
            <characteristic name="装甲值" typeId="83f8-a458-93f9-3e46">15</characteristic>
