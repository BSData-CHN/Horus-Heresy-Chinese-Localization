    <profile id="c3bb10b0-0ad1-3d7e-5b6e-20b2f57fbaba" name="干扰弹发射器" publicationId="ca571888--pubN67227" page="90" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="装备">
      <characteristics>
        <characteristic name="描述" typeId="4465736372697074696f6e23232344415441232323">Gains 4++ invulnerable against damage caused by missile weapons. </characteristic>
      </characteristics>
    </profile>
