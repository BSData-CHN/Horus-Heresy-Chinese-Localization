        <profile id="3386-3504-a10c-4c75" name="3. Shockwave" publicationId="ca571888--pubN106502" page="187" hidden="false" typeId="9c33-b0c8-74bd-e5a7" typeName="攻击灵能">
          <characteristics>
            <characteristic name="次元充能" typeId="c1b6-4261-dee4-923a">1</characteristic>
